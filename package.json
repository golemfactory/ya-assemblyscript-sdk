    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.5",
    "assemblyscript": "^0.17.0",
    "nodemon": "^2.0.6",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "@types/jasmine": "^3.6.0",
    "@types/node-fetch": "^2.5.7",
    "@types/uuid": "^8.3.0",
    "commander": "^6.2.0",
    "jasmine": "^3.6.2",
    "jszip": "^3.5.0",
    "node-fetch": "^2.6.1",
    "sha3": "^2.1.3",
    "uuid": "^8.3.1"
  }
